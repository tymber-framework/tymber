{
  "name": "@tymber/client",
  "version": "0.0.1",
  "description": "Client module for the Tymber framework",
  "license": "MIT",
  "author": "Damien ARRACHEQUESNE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tymber-framework/tymber.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "rm -rf dist/ && tsc",
    "format:check": "prettier -c src/",
    "format:fix": "prettier -w src/",
    "test": "exit 0"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "exports": {
    "node": "./dist/index.js",
    "bun": "./src/index.ts",
    "deno": "./src/index.ts"
  },
  "keywords": [
    "tymber",
    "typescript",
    "framework"
  ]
}
